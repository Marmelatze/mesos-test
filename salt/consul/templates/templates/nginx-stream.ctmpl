{{range ls "proxy"}}
{{with $d := .Value | parseJSON}}
    {{if eq $d.type "tcp"}} 
    upstream backend-{{$d.service}}-{{$d.servicePort}} {
        {{range service (print $d.service ":" $d.containerPort)}}
        server {{.Address}}:{{.Port}};
        {{end}}
    }
    server {
        listen {{$d.servicePort}};
        proxy_pass backend-{{$d.service}}-{{$d.servicePort}};
    }
    {{end}}
{{end}}
{{end}}
